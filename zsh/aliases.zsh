alias reload!='. ~/.zshrc'
alias old_gcc='export CXX=/usr/bin/g++-5.4'

alias cls='clear'
alias help='tldr'
alias serve='python -m http.server'
unsetopt auto_pushd
unsetopt pushd_ignore_dups

alias ls='exa'
alias tree='exa -T'
alias ping='./prettyping --nolegend'
alias du="ncdu --color dark -rr -x --exclude .git --exclude node_modules"

# Not add to history
alias t=' trans'
alias ag=' ag'

